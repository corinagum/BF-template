{
  "name": "bot-offline",
  "version": "0.0.0-0",
  "description": "Bot Framework Browser (Offline) Template",
  "main": "lib/index.js",
  "typings": "lib/DirectLineTypes.d.ts",
  "author": "corinagum",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/corinagum/BF-template"
  },
  "scripts": {
    "build": "npm run build:babel && npm run build:typescript",
    "build:babel": "babel --extensions .js,.ts --out-dir lib/ src/",
    "build:typescript": "tsc --build src",
    "start": "npm run build -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.13.0",
    "concurrently": "^7.5.0",
    "esbuild": "^0.16.15",
    "serve": "^14.1.2"
  },
  "dependencies": {
    "botbuilder-core": "^4.19.0",
    "core-js": "^3.27.1",
    "math-random": "^2.0.1"
  }
}
